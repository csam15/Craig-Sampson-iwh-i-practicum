doctype html 
html 
head   
    title= `${title}`
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(rel='stylesheet', href="public\style.css")

body 
    h1 Custom Object Table 
    a(href='update/cobj') Add/Update this table 
    table 
        thead   
            tr 
                th Name 
                th Brand 
                th Color 

        tbody 
            each item in data
                tr  
                    td= item.properties.name || 'NA'
                    td= item.properties.brand || 'NA'
                    td= item.properties.color || 'NA'